-- إعداد واجهة المستخدم
local screenGui = Instance.new("ScreenGui")
screenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

-- إعداد الـ Frame الرئيسي
local frame = Instance.new("Frame")
frame.Parent = screenGui
frame.Size = UDim2.new(0, 300, 0, 250) -- تكبير الإطار قليلاً
frame.Position = UDim2.new(0.5, -150, 1, 0) -- يبدأ خارج الشاشة (أسفل)
frame.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
frame.BorderSizePixel = 2
frame.Draggable = true -- جعل الواجهة قابلة للسحب
frame.Selectable = true
frame.Active = true

-- تحريك الواجهة عند فتحها (Animation)
game:GetService("TweenService"):Create(
    frame,
    TweenInfo.new(0.5, Enum.EasingStyle.Quint, Enum.EasingDirection.Out),
    {Position = UDim2.new(0.5, -150, 0.4, 0)}
):Play()

-- زر "Teleport 1" إلى الإحداثيات -37, 101, 37
local teleportButton1 = Instance.new("TextButton")
teleportButton1.Parent = frame
teleportButton1.Size = UDim2.new(0.8, 0, 0, 40)
teleportButton1.Position = UDim2.new(0.1, 0, 0.1, 0)
teleportButton1.Text = "Teleport 1"
teleportButton1.BackgroundColor3 = Color3.fromRGB(100, 100, 255)
teleportButton1.TextSize = 20

-- زر "Teleport 2" إلى الإحداثيات -34, 101, 26
local teleportButton2 = Instance.new("TextButton")
teleportButton2.Parent = frame
teleportButton2.Size = UDim2.new(0.8, 0, 0, 40)
teleportButton2.Position = UDim2.new(0.1, 0, 0.3, 0)
teleportButton2.Text = "Teleport 2"
teleportButton2.BackgroundColor3 = Color3.fromRGB(255, 100, 100)
teleportButton2.TextSize = 20

-- زر "Teleport 3" إلى الإحداثيات -34, 101, 12
local teleportButton3 = Instance.new("TextButton")
teleportButton3.Parent = frame
teleportButton3.Size = UDim2.new(0.8, 0, 0, 40)
teleportButton3.Position = UDim2.new(0.1, 0, 0.5, 0)
teleportButton3.Text = "Teleport 3"
teleportButton3.BackgroundColor3 = Color3.fromRGB(100, 255, 100)
teleportButton3.TextSize = 20

-- نص "By Drakthon"
local textLabel = Instance.new("TextLabel")
