tiktokLabel.TextSize = 14

-- إضافة رابط Discord في الزاوية اليمنى السفلية بخط شفاف
local discordLinkLabel = Instance.new("TextLabel")
discordLinkLabel.Parent = frame
discordLinkLabel.Size = UDim2.new(0.8, 0, 0, 20)
discordLinkLabel.Position = UDim2.new(0.55, 0, 0.92, 0)
discordLinkLabel.Text = "https://discord.gg/W3VwkUj6kf"
discordLinkLabel.BackgroundTransparency = 1
discordLinkLabel.TextColor3 = Color3.fromRGB(200, 200, 200) -- شفاف قليلاً
discordLinkLabel.TextSize = 14

-- إحداثيات النقل
local teleportPosition1 = Vector3.new(-37, 101, 37)
local teleportPosition2 = Vector3.new(-34, 101, 26)
local teleportPosition3 = Vector3.new(-34, 101, 12)

-- وظيفة النقل الفوري بسرعة غير طبيعية
local function teleportToPosition(position)
    local player = game.Players.LocalPlayer
    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        local hrp = player.Character.HumanoidRootPart
        local originalPosition = hrp.CFrame -- حفظ الموقع الأصلي
        
        -- النقل بسرعة غير طبيعية عبر حلقة لتغيير الـ CFrame بشكل متسلسل
        for _ = 1, 50 do -- تكرار النقل 50 مرة لجعل السرعة عالية
            hrp.CFrame = CFrame.new(position)
        end
        
        -- العودة إلى نفس المكان بعد تأخير بسيط
        task.wait(0.05) -- تقليل وقت الانتظار لجعل النقل فوري
        hrp.CFrame = originalPosition -- العودة إلى نفس الموقع الأصلي
    else
        warn("لا يوجد HumanoidRootPart في الشخصية!")
    end
end

-- ربط الزر الأول بوظيفة النقل إلى الإحداثيات الأولى
teleportButton1.MouseButton1Click:Connect(function()
    teleportToPosition(teleportPosition1)
end)

-- ربط الزر الثاني بوظيفة النقل إلى الإحداثيات الثانية
teleportButton2.MouseButton1Click:Connect(function()
    teleportToPosition(teleportPosition2)
end)

-- ربط الزر الثالث بوظيفة النقل إلى الإحداثيات الثالثة
teleportButton3.MouseButton1Click:Connect(function()
    teleportToPosition(teleportPosition3)
end)

-- ربط زر "Join Our Discord"
discordButton.MouseButton1Click:Connect(function()
    print("Join our Discord: https://discord.gg/W3VwkUj6kf")
end)
